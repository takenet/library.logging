          <add name="LogListeners" value="Takenet.Library.Logging.LogConsumer.Listeners.EntityFrameworkLogListener, Takenet.Library.Logging.LogConsumer;Takenet.Library.Logging.LogConsumer.Listeners.HttpLogListener, Takenet.Library.Logging.LogConsumer" />
          <add name="EntityFrameworkLogListener.ConnectionStringName" value="LoggingConnectionString" />
          <add name="HttpLogListener.Url" value="http://logs-01.loggly.com/inputs/18d6ea7d-981d-4990-95ef-916ad2756191/tag/http-client/" />
        </properties>
      </application>
      <!--<application name="Chat" consumersCount="1" listenerType="Takenet.Library.Logging.LogConsumer.Listeners.AggregateLogListener, Takenet.Library.Logging.LogConsumer">
        <properties>
          <add name="LogListeners" value="Takenet.Library.Logging.LogConsumer.Listeners.MongoDBLogListener, Takenet.Library.Logging.LogConsumer;Takenet.Library.Logging.LogConsumer.Listeners.EntityFrameworkLogListener, Takenet.Library.Logging.LogConsumer" />
          <add name="MongoDBLogListener.ConnectionStringName" value="MongoLoggingConnectionString" />
          <add name="MongoDBLogListener.CollectionSize" value="1000000000" />
          <add name="EntityFrameworkLogListener.ConnectionStringName" value="LoggingConnectionString" />
        </properties>
      </application>-->
    </applications>
  </logConsumerConfiguration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="EntityFramework" publicKeyToken="b77a5c561934e089" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v11.0" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
